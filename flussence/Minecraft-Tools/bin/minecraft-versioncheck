#!/usr/bin/env perl6

sub MAIN() {
    use Net::Minecraft::Version:ver<2+>;

    say q{Finding latest downloads...};

    with get-versions()<latest> {
        for .kv -> $type, $ver {
            say "$ver is the newest $type ({server-jar-for($ver)})";
        }
    }
    else {
        say q{Didn't find anything!};
    }
}
